		"@typescript-eslint/eslint-plugin": "6.11.0",
		"@typescript-eslint/parser": "6.11.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.31",
		"prettier": "3.1.0",
		"prettier-plugin-svelte": "3.1.0",
		"svelte": "4.2.3",
		"svelte-check": "3.6.0",
		"tailwindcss": "^3.3.5",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "5.0.0"
	},
	"type": "module",
	"dependencies": {
